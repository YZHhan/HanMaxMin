        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".hantext.activity.ImageSwitcherActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.listview.Main2Activity">
        </activity>
        <activity android:name=".activity.Main3Activity">
        </activity>

        <activity android:name=".activity.listview.recycler.RecyclerActivity">
        </activity>

        <activity android:name=".activity.viewpager.ViewPagerActivity">
        </activity>
    </application>

</manifest>