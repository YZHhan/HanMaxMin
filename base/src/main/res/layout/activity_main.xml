<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/ll_linear"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context="com.han.hanmaxmin.activity.MainActivity">

    <TextView
        android:id="@+id/thread_main"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="点击保存数据库"
        />

    <TextView
        android:id="@+id/thread_http"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="点击读取数据库"
        />

    <TextView
        android:id="@+id/thread_work"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="Thread_Work"
        />

    <TextView
        android:id="@+id/thread_single"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="Thread_Single"
        />
    <TextView
        android:id="@+id/tv_intent"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="点击跳转"
        android:gravity="center"

        />

</LinearLayout>
