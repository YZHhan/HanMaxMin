<?xml version="1.0" encoding="utf-8"?>
<merge
    android:id="@+id/mhvp_rootView"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.han.hanmaxmin.common.widget.viewpager.HanViewPager
        android:id="@+id/mhvp_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.han.hanmaxmin.common.widget.viewpager.headerpager.base.TranslatableLinearLayout
        android:id="@+id/mhvp_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:orientation="vertical">

        <com.han.hanmaxmin.common.widget.viewpager.headerpager.base.SizeSensitiveLinearLayout
            android:id="@+id/mhvp_headerCustom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"/>

    </com.han.hanmaxmin.common.widget.viewpager.headerpager.base.TranslatableLinearLayout>
</merge>